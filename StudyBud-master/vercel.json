{
  "version": 2,
  "builds": [
    {
      "src": "studybud/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "buildCommand": "pip install -r requirements.txt -t . && python -m compileall ."
      }
    },
    {
      "src": "staticfiles_build",  // assuming this is the output directory for your static build
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/studybud/wsgi.py"
    }
  ]
}

